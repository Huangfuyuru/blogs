{
    "title": "夏炜轩的笔记",
    "description": "夏炜轩的笔记",
    "author": "夏炜轩",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "links": {
        "sidebar": {
            "首页": "https://note.xiawx.top"
        }
    },
    "plugins": [
        "code",
        "-search",
        "search-pro",
        "github",
        "splitter",
        "donate",
        "-sharing",
        "sharing-plus",
        "-highlight",
        "styles-less",
        "toggle-chapters",
        "multipart",
        "ancre-navigation",
        "expandable-chapters",
        "advanced-emoji",
        "page-toc-button",
        "pageview-count",
        "back-to-top-button",
        "bsz"
    ],
    "pluginsConfig": {
        "page-toc-button": {
            "maxTocDepth": 2,
            "minTocSize": 2
        },
        "github": {
            "url": "https://github.com/xiaweixuan"
        },
        "code": {
            "copyButtons": true
        },
        "donate": {
            "wechat": "/assets/images/wxpay.png",
            "alipay": "/assets/images/wxpay.png",
            "title": "",
            "button": "赞赏",
            "alipayText": "一个是微信打赏",
            "wechatText": "另一个也是微信打赏"
        },
        "sharing": {
            "facebook": true,
            "twitter": true,
            "weibo": true,
            "qq": true,
            "all": [
                "douban",
                "google",
                "qzone",
                "linkedin"
            ]
        },
        "bsz": {
            "copyright": "2018&nbsp;-&nbsp;<span class=\"bsz-this-year\"></span>&nbsp;by&nbsp;<a href=\"https://note.xiawx.top\">夏炜轩</a>.&nbsp;All rights reserved."
        }
    },
    "styles": {
        "website": "assets/styles/website.css"
    }
}